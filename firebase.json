{
  "hosting": {
    "target":"bukie",
    "public": "main",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/home",
        "destination": "/index.html"
      },
      {
        "source": "/privacy",
        "destination": "/privacypolicy.html"
      }
    ]
  }

}
